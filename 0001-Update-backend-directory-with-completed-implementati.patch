+    },
+    "health_data": {
+        "name": "Health Information",
+        "description": "Protected health information (PHI)",
+        "classification_level": DataClassificationLevel.HIGHLY_RESTRICTED.value,
+        "data_patterns": json.dumps([
+            r"medical.record",
