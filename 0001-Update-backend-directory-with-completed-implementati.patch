+    scope: Optional[str] = None
+
+@dataclass
+class AccessToken:
+    """Access Token metadata"""
+    token: str
+    client_id: str
+    user_id: str
+    scope: List[str]
+    expires_at: datetime
+    created_at: datetime = field(default_factory=datetime.utcnow)
